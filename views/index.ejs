
<html>
    <head>
        <title> Message Board </title>
    </head>
    <body>
        <div>
            <% messages.forEach( msg => { %>
                <div>
                    <h2><%= msg.user %></h2>
                    <p> <%= msg.added %></p>
                    <p><%= msg.text %></p>
                </div>
            <% }) %> 

            <a href="/new">New Message</a> 
        </div>

    </body>
</html>
